#include<iostream>
using namespace std;
void main()
{
	const int size = 5;
		char grade [size];
		int counter = 0;
		for (int i = 0; i < size; i++)
		{
			cin >> grade[i];
			cout << "x[" << i << "]=" << endl;
		}
		
		for (int i = 0; i < size; i++)
			if (grade[i] == 'a' || grade[i] == 'A')
			{
				counter++;
			}
				
			{
				cout << "the number of student who got excellent is "<< counter <<endl;
			}
			

		
}
