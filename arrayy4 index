#include<iostream>
using namespace std;
void main()
{
	const int size = 5;
	char grade[size];
	int x = 0;
	int index = 0;

	for (int i = 0; i < size; i++)
	{
		cin >> grade[i];
		cout << "x[" << i << "]=" << endl;
	}

	for (int i = 0; i < size; i++) {
		if (grade[i] == 'f' || grade[i] == 'F')
		{
			x = 1;
			index = 1;
			
				cout << "found at " << i << endl;
		}
	}
	if (x == 0)
	{
		cout << "notfound" << endl;
	}
	
}
