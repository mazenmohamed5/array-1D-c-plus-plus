#include<iostream>
using namespace std;
void main()
{
	const int size = 5;
	int x[size];
	int max =0 ;
	for (int i = 0;i < size;i++)
	{
		cin >> x[i];
		cout << "x[" << i << "]=" << x[i] << endl;
	}

	for (int i = 0;i < size;i++)
		if(x[i]> max )
			max = x[i];
	     
	{
		cout << "the max value is " << max << endl;
	}

}
